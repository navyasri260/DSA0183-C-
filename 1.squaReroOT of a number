#include <iostream>
#include <cmath>  

int main() {
    double number, squareRoot;
    std::cout << "Enter a number: ";
    std::cin >> number;
    if (number >= 0) {
        squareRoot = sqrt(number);
        std::cout << "Square root of " << number << " is " << squareRoot << std::endl;
    } else {
        std::cout << "Error: Cannot compute square root of a negative number." << std::endl;
    }

    return 0;
}
